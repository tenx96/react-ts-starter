{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "Template webpack project for react with typescript including linting, husky,prettier integration",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --node-env=production --config webpack.prod.js",
    "build:dev": "webpack --mode=development --config webpack.dev.js" ,
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack serve --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "lint": "eslint 'src/**/*.tsx'",
    "lint:fix": "eslint 'src/**/*.tsx' --fix",
    "lint:quiet": "eslint 'src/**/*.tsx' --quiet",
    "prepare": "husky install",
    "test": "jest --watchAll"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.3.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "css-loader": "^6.6.0",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "lint-staged": {
    "*.tsx": "eslint --cache --fix"
  }
}
